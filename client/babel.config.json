{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead",
        "useBuiltIns": "usage",
        "corejs": { "version": 3, "proposals": true },
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["@emotion", "@babel/plugin-proposal-class-properties"]
}
